class Solution:
    # @param A : integer
    # @param B : integer
    # @return an integer
    def solve(self, A, B):
        X=0
        for i in range(30,-1,-1):
            if(B==0):
                break
            if((A&(1<<i))>0):
                X|=(1<<i)
                B-=1

        for i in range(0,31):
            if(B==0):
                break
            if((A&(1<<i))==0):
                X|=(1<<i)
                B-=1

        return X

            



